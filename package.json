{
  "name": "singularity-convert",
  "displayName": "Singularity Convert",
  "description": "Extensión de ejemplo para mostrar un panel con un textbox y un botón.",
  "version": "1.0.2",
  "publisher": "jeffrycascante",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffrycascate/singularity-convert.git"
  },
  "main": "extension.js",
  "activationEvents": [
    "onView:myView"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "singularityEditor",
          "title": "Singularity Editor",
          "icon": "media/icono.svg"
        }
      ]
    },
    "views": {
      "singularityEditor": [
        {
          "type": "webview",
          "id": "myView",
          "name": "Singularity View"
        }
      ]       
    }
  },
  "devDependencies": {
    "vscode": "^1.60.0"
  } 
}
